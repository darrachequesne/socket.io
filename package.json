{
  "name": "socket.io",
  "version": "1.3.7",
  "description": "node.js realtime framework server",
  "keywords": [
    "realtime",
    "framework",
    "websocket",
    "tcp",
    "events",
    "socket",
    "io"
  ],
  "main": "./dist/index",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Automattic/socket.io"
  },
  "scripts": {
    "test": "npm run compile && mocha --reporter dot --slow 200ms --bail",
    "compile": "babel -d dist/ lib/",
    "prepublish": "npm run compile"
  },
  "dependencies": {
    "engine.io": "automattic/engine.io#master",
    "socket.io-parser": "2.2.4",
    "socket.io-client": "automattic/socket.io-client#master",
    "socket.io-adapter": "automattic/socket.io-adapter#master",
    "has-binary": "0.1.7",
    "debug": "2.1.3",
    "babel-runtime": "6.1.18"
  },
  "devDependencies": {
    "expect.js": "0.3.1",
    "istanbul": "0.2.3",
    "mocha": "2.3.4",
    "superagent": "0.17.0",
    "supertest": "0.8.2",
    "babel-cli": "6.1.18",
    "babel-preset-es2015": "6.1.18",
    "babel-plugin-transform-runtime": "6.1.18"
  },
  "contributors": [
    {
      "name": "Guillermo Rauch",
